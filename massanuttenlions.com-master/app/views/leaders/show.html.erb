<% provide :page_title, "Leadership" %>
<div class="page-header">
  <h1>Leadership <%= @current_year ? @current_year.current_year : "" %></h1>
</div>
<ul class="thumbnails">
<% @users.each do |user| %>
          <li>
            <div class="thumbnail center leadership">
              <%= image_tag user.avatar.url(:medium) %>
              <h6> <%= user.leadership.upcase %> <br><%= user.signature %></h6>
            </div>
          </li> <p ><%= user.biography %></p>
      <div class="clearfix"></div>  <hr>
      <% end %>
  <% @users_officers.each do |user| %>
      <li>
        <div class="thumbnail center leadership">
          <%= image_tag user.avatar.url(:medium) %>
          <h6><%= user.officers.upcase %> <br><%= user.signature %></h6>
        </div>
      </li> <p ><%= user.biography %></p>
      <div class="clearfix"></div>  <hr>
  <% end %></ul>


