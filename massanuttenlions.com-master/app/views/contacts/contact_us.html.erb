<% provide :page_title, "Contact Us" %>
<div id="map_canvas"></div>
<div class="page-header">
  <h1>Contact Us</h1>
</div>
<% unless @club.nil? %>
    <h3>Meetings Address:</h3>
    <address>


      <strong><%= @club.name %></strong><br>
      <%= @club.meeting_address %>
      <br>
    </address>
    <address><strong>Phone:</strong> <%= @president.phone %> <br>
      <strong>Email Address: </strong> <a href="mailto:#" target="_blank"><%= @club.email %></a></address>
    <h3>Mailing Address:</h3>
    <address>
      <strong><%= @club.name %></strong><br>
      <%= @club.mailing_address %>

<% else %>
    <p class="alert alert-error">No data available!</p>
<% end %>

</address>
